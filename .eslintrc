{
    "parser": "@typescript-eslint/parser",
    "env": {
        "browser": true,
        "node": true,
        "mocha": true
    },
    "extends": "naver",
    "rules": {
        "linebreak-style": 0,
        "consistent-this": 0,
        "no-nested-ternary": 0,
        "object-shorthand": 0,
        "comma-dangle": 0,
        "no-lonely-if": 0,
        "no-shadow": 0,
        "default-case": 0,
        "class-methods-use-this": 0,
        "no-useless-escape": 0,
        "no-loop-func": 0,
        "no-console":[
            "error", {
                "allow": ["error"]
            }
        ],
        "array-element-newline": 0,
        "import/prefer-default-export": 0
    }
}

{
    "compilerOptions": {
        "module": "commonjs",
        "noImplicitAny": false,
        "noImplicitThis": true,
        "strictNullChecks": true,
        "strictFunctionTypes": false,
        "baseUrl": "../",
        "lib": [
            "es6", "dom"
        ],
        "typeRoots": [
            "./node_modules/@types"
        ]
    },
    "include": [
        "./*.ts"
    ],
    "noEmit": true,
    "forceConsistentCasingInFileNames": true
}
